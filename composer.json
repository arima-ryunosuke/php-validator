{
  "config": {
    "platform": {
      "php": "8.0"
    },
    "process-timeout": 0
  },
  "name": "ryunosuke/chmonos",
  "type": "library",
  "description": "Compatible php/js validator library",
  "license": "MIT",
  "authors": [
    {
      "name": "ryunosuke",
      "email": "ryunosuke.arima@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.0",
    "symfony/polyfill-php82": "*"
  },
  "require-dev": {
    "ryunosuke/functions": "2.0.*",
    "phpunit/phpunit": "9.*",
    "ryunosuke/phpunit-extension": "4.*",
    "php-webdriver/webdriver": "1.*"
  },
  "autoload": {
    "psr-4": {
      "ryunosuke\\chmonos\\": "src/chmonos"
    },
    "files": [
      "src/functions.stub"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "ryunosuke\\Test\\": "tests/Test"
    },
    "files": [
      "tests/functions.php"
    ]
  },
  "scripts": {
    "build": [
      "@export",
      "@npm",
      "@test",
      "@output"
    ],
    "export": [
      "export-function",
      "exclude-internal"
    ],
    "npm": "@php bin/npm.php",
    "output": "@php bin/output.php",
    "test": "vendor/bin/phpunit --configuration=tests/phpunit.xml",
    "post-install-cmd": [
      "@export"
    ],
    "post-update-cmd": [
      "@export"
    ]
  }
}
